importar guessNumber como modulo
funcao pedirPalpite():
    entrada("Digite um valor inteiro entre 0 e 100: ", valor)
    retorne valor

programa = modulo.GuessNumber() # criar uma intância do modelo GuessNumber
palpite = -1
enquanto palpite != programa.numeroGerado
    palpite = pedirPalpite()
    imprime(programa.devolverPista(palpite))

imprime("Acertou! Parabéns!")